function start() {
makeRightSquare();
}
function makeRightSquare() {
    putBall();
    move();
    putBall();
    turnLeft();
    move();
    putBall();
    turnLeft();
    move();
    putBall();
    turnLeft();
    move();
    turnLeft();
}
function makeLeftSquare() {
    putBall();
    turnLeft();
    move();
    putBall();
    turnLeft();
    turnLeft();
    turnLeft();
    move();
    putBall();
    turnLeft();
    turnLeft();
    turnLeft();
    move();
    putBall();
    turnLeft();
    
}
//unused makeLeftSquare();
